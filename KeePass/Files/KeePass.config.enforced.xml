<?xml version="1.0" encoding="utf-8"?>
<Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <Application>
        <Start>
            <CheckForUpdate>false</CheckForUpdate>
            <CheckForUpdateConfigured>true</CheckForUpdateConfigured>
        </Start>
        <UseFileLocks>false</UseFileLocks>
    </Application>
	<Security>
		<WorkspaceLocking>
			<LockOnSessionSwitch>true</LockOnSessionSwitch>
            <LockOnSuspend>true</LockOnSuspend>
			<LockOnRemoteControlChange>true</LockOnRemoteControlChange>
            <LockAfterGlobalTime>300</LockAfterGlobalTime>
		</WorkspaceLocking>
        <Policy>
			<Export>false</Export>
			<ExportNoKey>false</ExportNoKey>
			<Print>false</Print>
			<PrintNoKey>false</PrintNoKey>
			<ChangeMasterKeyNoKey>false</ChangeMasterKeyNoKey>
        </Policy>
		<MasterPassword>
			<MinimumLength>10</MinimumLength>
			<MinimumQuality>0</MinimumQuality>
		</MasterPassword>
        <ClipboardClearOnExit>true</ClipboardClearOnExit>
        <ClipboardClearAfterSeconds>12</ClipboardClearAfterSeconds>
        <SslCertsAcceptInvalid>false</SslCertsAcceptInvalid>
        <UseClipboardViewerIgnoreFormat>false</UseClipboardViewerIgnoreFormat>
	</Security>
    <UI>
        <UIFlags>32</UIFlags>
    </UI>
    <Integration>
    	<SearchKeyFiles>false</SearchKeyFiles>
	<LimitToSingleInstance>true</LimitToSingleInstance>
        <AutoTypeCancelOnWindowChange>true</AutoTypeCancelOnWindowChange>
	<AutoTypeCancelOnTitleChange>true</AutoTypeCancelOnTitleChange>
    </Integration>
</Configuration>